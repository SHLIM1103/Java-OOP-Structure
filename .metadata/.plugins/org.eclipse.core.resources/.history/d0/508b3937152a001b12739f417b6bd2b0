package viewer;

public class LoopTest {
	public static void main(String[] args) {
		// 1 + 2 = 3
		for (int i = 0; i < 10; i++) {
			System.out.printf("%d + ", i+1);
			i += i;
			int sum = 0 ;
			sum += i;
			System.out.println(sum);
		}
		
		// 1 + 2 = 3
		int i = 0;
		while (i < 10) {
			i++;
			
		}
	}
}